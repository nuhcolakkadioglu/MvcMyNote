@model List<MyNote.Enties.Messages.ErrorMessageObj>
@{
    ViewBag.Title = "Hesap geçersiz";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/Login.css" rel="stylesheet" />

<section class="login-info">
    <div class="container">
        <div class="row main">
            <div class="form-header">
                <h1 class="text-center ">Yönlendiriliyorsunuz</h1>
            </div>
            <div class="main-content">

                <div class="text-danger">
                    <h2><span class="glyphicon glyphicon-remove"></span> Hesap geçersiz</h2>
                    <hr />
                    <p class="text-justify">
                       
                        @foreach (var item in Model)
                        {
                            <p>@item.Message</p><br />
                        }


                    </p>
                </div>

            </div>
        </div>
    </div>
</section>

<script>
    $(function () {
        $('footer').hide();
        setTimeout(function () {
            window.location = "/Home/Index";
        }, 10000);
    });
</script>